INÍCIO(seed,input,output)

    NÚMERO n
    ESTADO st

	ABRE input
	REPETE {
        LÊ_LINHA e->n

        VECTOR v[n]
        MONITOR_ESTADO("iniciar")

        ORDENA(v)  ::Estudar isto.

        st <- MONITOR_ESTADO("encerrar")

        IMPRIME_ESTADO st->o

        TESTA_PIOR_CASO

	} ATÉ EOF

FIM
